page{
  width: 100%;
  height: 100%;
}
.test-bg{
  width: 100%;
  height: 100%;
  position: relative;
}
.bg-image{
  display: block;
  width: 100%;
  height: 100%;
}
input{
  width: 350rpx;
  color: #ffffff;
  padding-top: 5rpx;
}
.login-title{
  color: #ffffff;
  position: absolute;
  top:150rpx;
  left: 225rpx;
  font-size: 200%;
}
.loginview{
  width: 100%;
  position: absolute;
  top:300rpx;
  color: #ffffff;
}
.login-line1,.login-line2{
  display: inline-flex;
  border: 1rpx solid #ffffff;
  border-radius: 30rpx;
  position: relative;
  left: 120rpx;
  font-size: 120%;
  margin-top: 50rpx;
  padding-top: 10rpx;
  padding-bottom: 10rpx;
}
.thetext{
  width: 150rpx;
  padding-left: 20rpx;
}
button{
  color: #ffffff;
  letter-spacing: 20rpx;
  border: none;
  border-radius: 20rpx;
  line-height:50rpx;
  padding-top: 10rpx;
  height: 70rpx;
  width: 520rpx;
  background-color: #33CCFF;
  margin-top: 100rpx;
  margin-left: 120rpx;
}
.tips{
  position: absolute;
  left: 250rpx;
  top: 750rpx;
  color: #FFFFFF;
  display: inline-flex;
}
