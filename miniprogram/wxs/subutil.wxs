var subname = function (val) {
  if (val.length == 0 || val == undefined) {
    return;
  } else {
    return val.substring(0, 1) + '‚ù§' + val.slice(-1);
  }
}
module.exports.subname = subname;

var subtime = function (val) {
  if (val.length == 0 || val == undefined) {
    return;
  } else {
    return val.slice(-8);
  }
}
module.exports.subtime = subtime;

